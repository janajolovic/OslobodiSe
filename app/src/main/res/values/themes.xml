<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OslobodiSeResi" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/plava</item>
        <item name="colorPrimaryVariant">@color/tamno_plava</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/narandzasta</item>
        <item name="colorSecondaryVariant">@color/tandzarina</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="colorBackgroundFloating">@color/platinum</item>
        <item name="android:textColor">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.OslobodiSeResi.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.OslobodiSeResi.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.OslobodiSeResi.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>